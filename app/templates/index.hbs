{{#each model as |item|}}
  <article class="listing">
    <h3>{{item.item_name}}</h3>
    <div class="detail owner">
      <span>User:</span> {{item.user_id}}
    </div>
    <div class="detail type">
      <span>Description:</span> {{item.item_desc}}
    </div>
    <div class="detail location">
      <span>Price:</span> {{item.item_price}}
    </div>
  </article>
{{/each}}
